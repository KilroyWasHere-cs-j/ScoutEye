<Window x:Class="ScoutEye.APIInterface"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScoutEye"
        mc:Ignorable="d"
        Title="APIInterface" Height="450" Width="800">
    <Grid Background="Black">
        <TextBox x:Name="CommandTB" HorizontalAlignment="Center" Margin="0,31,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="715"/>
        <Button x:Name="RunCommandBTN" Content="Run Command" HorizontalAlignment="Left" Margin="42,97,0,0" VerticalAlignment="Top" Height="54" Width="134" Click="RunCommandBTN_Click"/>
        <Button x:Name="OpenFileBTN" Content="Open Log File" HorizontalAlignment="Left" Margin="42,179,0,0" VerticalAlignment="Top" Height="54" Width="134" Click="OpenFileBTN_Click"/>
        <TextBox x:Name="APIOutputTB" HorizontalAlignment="Left" Margin="201,78,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="557" Height="331" IsReadOnly="True"/>
    </Grid>
</Window>
